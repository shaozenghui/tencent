.container{
    width: 690rpx;
    overflow: hidden;
    padding: 30rpx;

}
.title{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}
.title text{
    float: left;
    color: #3da8f5
}
.title button{
    float: right;
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #3da8f5;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
}
.content{
    width: 650rpx;
    height: 260rpx;
    border: 1rpx solid #cacaca;
    border-radius: 10rpx;
    padding: 20rpx;
    margin-bottom: 20px;
}
.navt{
    width: 100%;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    margin: 80rpx 0;
    font-size: 40rpx;
    color: #3da8f5;
}