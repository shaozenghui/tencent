.container{
    width: 690rpx;
    overflow: hidden;
    padding: 30rpx;
    margin-top: 50rpx;

}
.title{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 50rpx;
}
.title text{
    float: left;
    color: #3da8f5;
}
.title switch{
    float: left;
      margin:0  20rpx;
}
.title button{
    float: right;
    width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #3da8f5;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
}
.content{
    width: 650rpx;
    height: 260rpx;
    border: 1rpx solid #3da8f5;
    border-radius: 10rpx;
    padding: 20rpx;
    margin-bottom: 30px;
    position: relative
}
.navt{
    width: 100%;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    margin: 80rpx 0;
    font-size: 40rpx;
    color: #3da8f5;
}
.pron{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    bottom: 20rpx;
    right: 20px;
    color: #3da8f5;
    font-size: 30rpx;
    z-index: 100;
}
.soun{
  width: 100%;
  height: 60rpx;
  position: relative;
}
.soun image{
    width: 50rpx;
    height: 60rpx;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -25rpx;
}